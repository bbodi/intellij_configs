    <keyboard-shortcut first-keystroke="ctrl alt m" />
    <keyboard-shortcut first-keystroke="ctrl comma" second-keystroke="m" />
  </action>
  <action id="IntroduceVariable">
    <keyboard-shortcut first-keystroke="ctrl alt v" />
    <keyboard-shortcut first-keystroke="ctrl comma" second-keystroke="v" />
  </action>
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="ctrl comma" second-keystroke="r" />
  </action>
</keymap>